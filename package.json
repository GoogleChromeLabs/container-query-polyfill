{
  "name": "container-query-polyfill",
  "version": "0.1.2",
  "description": "",
  "type": "module",
  "main": "dist/src/cqfill.js",
  "module": "dist/src/cqfill.js",
  "files": [
    "README.md",
    "LICENSE",
    "dist/src/*",
    "package.json"
  ],
  "scripts": {
    "build:clean": "rimraf ./dist/",
    "build:iife:minified": "esbuild --bundle ./dist/src/cqfill.js --outfile=./dist/src/cqfill.iife.min.js --format=iife --minify",
    "build:esm:minified": "esbuild --bundle ./dist/src/cqfill.js --outfile=./dist/src/cqfill.min.js --format=esm --minify",
    "build:tsc": "tsc",
    "build": "npm run build:tsc && npm run build:esm:minified && npm run build:iife:minified",
    "lint": "eslint '**/*.ts'",
    "prettier:fix": "prettier --write .",
    "serve": "superstatic -p 9606 ."
  },
  "author": "Google Chrome Developers <chromium-dev@google.com>",
  "contributors": [
    "Surma <surma@surma.dev>"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "esbuild": "^0.14.31",
    "eslint": "^8.12.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "superstatic": "^8.0.0",
    "typescript": "^4.6.3"
  }
}
